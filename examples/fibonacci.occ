fn fib_while(i64 n) i64 {
  i64 n1 = 0;
  i64 n2 = 1;
  
  i64 i = 0;
  while i < n {
    i64 next = n1 + n2;
    n1 = n2;
    n2 = next;
    
    i = i + 1;
  }
  
  return n1;
}

fn main() i64 {
  return fib_while(12);
}
