include "../lib/stdio.occ"

struct point_2d {
    i64 x;
    i64 y;
}

struct point_3d {
    point_2d p_2d;
    i64 z;
}

fn create_3d_point(i64 x, i64 y, i64 z) point_3d {
    point_3d p;

    p.p_2d.x = x;
    p.p_2d.y = y;
    p.z = z;

    return p;
}

fn print_point3d(point_3d p) {
    print_integer(p.p_2d.x);
    print_string("\n");

    print_integer(p.p_2d.y);
    print_string("\n");

    print_integer(p.z);
    print_string("\n");
}

fn main() {
    point_3d p = create_3d_point(10, 20, 30);

    print_point3d(p);

    return 0;
}
