include "../lib/stdio.occ"

fn main() i64 {
    i64 orig = 10;

    i64* addr = @orig; // reference
    i64** addr2 = @addr;
    
    $$addr2 = 40; // how to dereference

    print_integer(addr); // raw address 
    print_string("\n");
    print_integer($addr); // 40
    print_string("\n");
    print_integer($$addr2); // 40
    print_string("\n");
    
    return 0;
}
