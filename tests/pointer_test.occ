fn main() i64 { // yay this works :3
    i64 orig = 10;

    i64* addr = @orig; // reference
    i64** addr2 = @addr;
    
    <-<-addr2 = 40;

    printn(addr);
    printn(<-addr);
    printn(<-<-addr2); // dereference 
    
    return 0;
}

