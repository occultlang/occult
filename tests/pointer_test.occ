fn main() i64 {
    i64 orig = 10;

    i64* addr = @orig; // reference
    i64** addr2 = @addr;
    
    $$addr2 = 40; // how to dereference

    print_i(addr); // raw address 
    print_s("\n");
    print_i($addr); // 40
    print_s("\n");
    print_i($$addr2); // 40
    print_s("\n");
    
    return 0;
}
