include "../lib/stdio.occ"

struct point {
    i64 x;
    i64 y;
}

fn create_point(i64 x, i64 y) point {
    point p;
    p.x = x;
    p.y = y;
    return p;
}

fn main() {
    point p = create_point(1, 2);
    print_integer(p.x);
    print_string(" ");
    print_integer(p.y);
    print_string("\n");

    p = create_point(10, 20);
    print_integer(p.x);
    print_string(" ");
    print_integer(p.y);
    print_string("\n");

    return 0;
}
