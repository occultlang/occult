include "../lib/printf.occ"

fn main() {
    printf("Integer: %d\n", 42);
    printf("String: %s\n", "hello");
    printf("Float default: %f\n", 100.0);
    printf("Float .2: %.2f\n", 100.0);
    printf("Float .4: %.4f\n", 100.0);
    printf("Negative: %.2f\n", -42.0);
    printf("Mixed: %s scored %.1f out of %d\n", "Alice", 95.0, 100);
}
