include "../lib/printf.occ"

fn sum_all(i64 count, ...) i64 {
    i64 total = 0;
    i64 i = 0;
    while i < count {
        total = total + __varargs[i];
        i = i + 1;
    }
    return total;
}

fn main() {
    printf("Sum of 3: %d\n", sum_all(3, 10, 20, 30));
    printf("Sum of 5: %d\n", sum_all(5, 1, 2, 3, 4, 5));
    printf("%d %d %d %d %d\n", 100, 200, 300, 400, 500);
    return 0;
}
